{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "components/*": ["src/components/*"],
      "constant/*": ["src/constant/*"],
      "styles/*": ["src/styles/*"], 
      "redux-store/*": ["src/redux-store/*"],
      "utils/*": ["src/utils/*"],
      "api/*": ["src/api/*"],
      "secure-store/*": ["src/secure-store/*"],
    }
  }
}
